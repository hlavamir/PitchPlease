          <Pad Id="V6KrQSIhWl8LAM5bDx6sUw" Bounds="109,217,434,129" ShowValueBox="true" isIOBox="true" Value="It means that the Data can be:&#xD;&#xA;1. prefixed with some sequence of bytes: [xxx]246.&#xD;&#xA;2. postfixed with some sequence of bytes: 246[xxx]&#xD;&#xA;3. pre- and postfixed with some sequence of bytes: [xxx]246[yyy]&#xD;&#xA;4. prefixed with several bytes holding the length of the data: [3]246&#xD;&#xA;5. always of fixed length">
            <p:TypeAnnotation LastCategoryFullName="Primitive" LastSymbolSource="CoreLibBasics.vl">
              <Choice Kind="TypeFlag" Name="String" />
            </p:TypeAnnotation>
            <p:ValueBoxSettings>
              <p:fontsize p:Type="Int32">9</p:fontsize>
              <p:stringtype p:Assembly="VL.Core" p:Type="VL.Core.StringType">Comment</p:stringtype>
            </p:ValueBoxSettings>
          </Pad>
          <Pad Id="SNf2PqYpRRwOVPmfgekEZY" Bounds="109,348,453,69" ShowValueBox="true" isIOBox="true" Value="The first 3 options are covered by the FrameBytes node.&#xD;&#xA;For the 4th one use the PrefixLength node.&#xD;&#xA;The 5th option doesn't introduce any special separator into the stream.">
            <p:TypeAnnotation LastCategoryFullName="Primitive" LastSymbolSource="CoreLibBasics.vl">
              <Choice Kind="TypeFlag" Name="String" />
            </p:TypeAnnotation>
            <p:ValueBoxSettings>
