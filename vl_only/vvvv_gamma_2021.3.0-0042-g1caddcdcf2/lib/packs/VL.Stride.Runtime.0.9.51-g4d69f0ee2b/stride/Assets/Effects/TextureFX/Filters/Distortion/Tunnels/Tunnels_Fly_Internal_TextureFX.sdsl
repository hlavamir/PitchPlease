shader Tunnels_Fly_Internal_TextureFX : TunnelsBase
{
	stage float3 Tunnel(float2 p)
	{
		float2 uv;
		float an = Rotate;
		float x = p.x*cos(an)-p.y*sin(an);
		float y = p.x*sin(an)+p.y*cos(an);  	
		uv.x = .25*x/abs(y)*Dist+(Offset+0.5);
		uv.y = .25*time + .25/abs(y)*Dist;	
		return float3(uv, y);
	}
};