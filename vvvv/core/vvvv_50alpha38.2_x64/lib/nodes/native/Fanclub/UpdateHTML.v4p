   <PIN pinname="Input" visible="1" slicecount="1" values="|&lt;html&gt;&cr;&lf;&lt;header&gt;&cr;&lf;  &lt;link href=&quot;irc.css&quot; rel=&quot;stylesheet&quot;&gt;&cr;&lf;  &lt;script&gt;&cr;&lf;    function connect()&cr;&lf;    {&cr;&lf;      var f = document.connection;&cr;&lf;&cr;&lf;      document.getElementById(&apos;Server&apos;).innerHTML = f.Server.value;&cr;&lf;      document.getElementById(&apos;Channel&apos;).innerHTML = f.Channel.value;&cr;&lf;      document.getElementById(&apos;Nickname&apos;).innerHTML = f.Nickname.value;&cr;&lf;      document.getElementById(&apos;Password&apos;).innerHTML = f.Password.value;&cr;&lf;      document.getElementById(&apos;Connect&apos;).innerHTML = 1;&cr;&lf;&cr;&lf;      focusTextfield();&cr;&lf;    }&cr;&lf;&cr;&lf;    function onload()&cr;&lf;    {&cr;&lf;      //scroll messages&cr;&lf;      var elem = document.getElementById(&apos;messages&apos;);&cr;&lf;      elem.scrollTop = elem.scrollHeight;&cr;&lf;&cr;&lf;      focusTextfield();&cr;&lf;    }&cr;&lf;&cr;&lf;    function  messageForm()&cr;&lf;    {&cr;&lf;      document.getElementById(&apos;Message&apos;).innerHTML = document.message.Message.value;&cr;&lf;      focusTextfield();&cr;&lf;    }&cr;&lf;&cr;&lf;    function focusTextfield()&cr;&lf;    {&cr;&lf;      //set focus to the message textfield&cr;&lf;      document.getElementById(&apos;MessageField&apos;).focus();&cr;&lf;    }&cr;&lf;&cr;&lf;&lt;/script&gt;&cr;&lf;&lt;/header&gt;&cr;&lf;&lt;body onLoad = &quot;onload()&quot;&gt;&cr;&lf;  &lt;div class=&quot;container&quot;&gt;&cr;&lf;    &lt;div class = &quot;header&quot;&gt;&cr;&lf;      &lt;form action=&quot;javascript:connect()&quot; name=&quot;connection&quot;&gt;&cr;&lf;      &lt;div class=&quot;connection&quot;&gt;&cr;&lf;        &lt;div class=&quot;hidden&quot;&gt;&lt;label&gt;Server: &lt;/label&gt;&lt;input type=&quot;text&quot; name=&quot;Server&quot; value =&quot;irc.freenode.net&quot; placeholder=&quot;Server&quot;&gt;&lt;/div&gt;&cr;&lf;        &lt;div class=&quot;hidden&quot;&gt;&lt;label&gt;Channel: &lt;/label&gt;&lt;input type=&quot;text&quot; name=&quot;Channel&quot; value =&quot;##sandbox&quot; placeholder=&quot;Channel&quot;&gt;&lt;/div&gt;&cr;&lf;        &lt;div class=&quot;&quot;&gt;&lt;label&gt;Nickname: &lt;/label&gt;&lt;input type=&quot;text&quot; name=&quot;Nickname&quot; value = &quot;vvvvan4028&quot; placeholder=&quot;Nickname&quot; &gt;&lt;/div&gt;&cr;&lf;        &lt;div class=&quot;hidden&quot;&gt;&lt;label&gt;Password: &lt;/label&gt;&lt;input type=&quot;text&quot; name=&quot;Password&quot; value = &quot;&quot; placeholder=&quot;(optional)&quot;&gt;&lt;/div&gt;&cr;&lf;      &lt;/div&gt;&cr;&lf;      &lt;div class = &quot;status&quot;&gt;&cr;&lf;        &lt;div class = &quot;connectionButton&quot;&gt;&lt;button&gt;Connect&lt;/button&gt;&lt;/div&gt;&cr;&lf;        &lt;div class = &quot;connectionBox green&quot;&gt;&lt;/div&gt;&lt;div class = &quot;connectionStatus&quot;&gt;Connected:&lt;/div&gt;&cr;&lf;        &lt;div class = &quot;connectionInfo&quot;&gt;irc.freenode.net, ##sandbox&lt;/div&gt;&cr;&lf;      &lt;/div&gt;&cr;&lf;    &lt;/form&gt;&cr;&lf;    &lt;/div&gt;&cr;&lf;    &lt;div class = &quot;main&quot;&gt;&cr;&lf;      &lt;div class = &quot;Error&quot;&gt;[error]&lt;/div&gt;&cr;&lf;          &lt;div class=&quot;messages&quot; id=&quot;messages&quot;&gt;&cr;&lf;&cr;&lf;            &cr;&lf; &cr;&lf;        &lt;/div&gt;&cr;&lf;          &lt;div class=&quot;buddylist&quot;&gt;&cr;&lf;            &lt;h3&gt;Users:&lt;/h3&gt;&cr;&lf;            &lt;div class = &quot;users&quot;&gt;&cr;&lf;              vvvvan4028&lt;br&gt;BlueCrab&lt;br&gt;PingChan&lt;br&gt;iorec&lt;br&gt;untone_&lt;br&gt;ChanServ&cr;&lf;            &lt;/div&gt;&cr;&lf;          &lt;h3&gt;Type:&lt;/h3&gt;&cr;&lf;            &lt;div class=&quot;info&quot;&gt;&cr;&lf;              @username to notify a user.&lt;br&gt;&cr;&lf;              @all to notify everybody.&cr;&lf;            &lt;/div&gt;&cr;&lf;            &lt;!-- &lt;a href=&quot;https://vvvv.org/documentation/howto-use-irc&quot;&gt;Click for more info.&lt;/a&gt; --&gt;&cr;&lf;        &lt;/div&gt;&cr;&lf;&cr;&lf;    &lt;/div&gt;&cr;&lf;&cr;&lf;    &lt;div class = &quot;sendForm&quot;&gt;&cr;&lf;        &lt;form action=&quot;javascript:messageForm()&quot; name = &quot;message&quot;&gt;&cr;&lf;          &lt;input type=&quot;text&quot; name=&quot;Message&quot; id = &quot;MessageField&quot;&gt;&cr;&lf;          &lt;button&gt;Send&lt;/button&gt;&cr;&lf;        &lt;/form&gt;&cr;&lf;        &lt;div class = &quot;rightSpace&quot;&gt;&cr;&lf;        &lt;/div&gt;&cr;&lf;    &lt;/div&gt;&cr;&lf;&cr;&lf;&cr;&lf;  &lt;/div&gt;&cr;&lf;&cr;&lf;  &lt;div class=&quot;data&quot; id=&quot;data&quot;&gt;&cr;&lf;    &lt;div id=&quot;Server&quot;&gt;&lt;/div&gt;&cr;&lf;    &lt;div id=&quot;Channel&quot;&gt;&lt;/div&gt;&cr;&lf;    &lt;div id=&quot;Nickname&quot;&gt;&lt;/div&gt;&cr;&lf;    &lt;div id=&quot;Password&quot;&gt;&lt;/div&gt;&cr;&lf;    &lt;div id=&quot;Connect&quot;&gt;&lt;/div&gt;&cr;&lf;    &lt;div id=&quot;Message&quot;&gt;&lt;/div&gt;&cr;&lf;  &lt;/div&gt;&cr;&lf;&lt;/body&gt;&cr;&lf;&lt;/html&gt;&cr;&lf;|">
