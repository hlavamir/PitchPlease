me: &lt;/label&gt;&lt;input type=&quot;text&quot; name=&quot;Nickname&quot; value = &quot;vvvvuser&quot; placeholder=&quot;Nickname&quot; &gt;&cr;&lf;        &lt;label&gt;Password: &lt;/label&gt;&lt;input type=&quot;text&quot; name=&quot;Password&quot; value = &quot;&quot; placeholder=&quot;(optional)&quot;&gt;&cr;&lf;        &lt;div class=&quot;connectionBox [statusColor]&quot;&gt;&lt;/div&gt;&cr;&lf;        &lt;div class=&quot;connectionStatus&quot;&gt;[status]&lt;/div&gt;&cr;&lf;        &lt;button&gt;Connect&lt;/button&gt;&cr;&lf;      &lt;/form&gt;&cr;&lf;    &lt;/div&gt;&cr;&lf;    &lt;div class = &quot;main&quot;&gt;&cr;&lf;      &lt;div class = &quot;Error&quot;&gt;[error]&lt;/div&gt;&cr;&lf;          &lt;div class=&quot;messages&quot;&gt;&cr;&lf;&cr;&lf;            &lt;!--Message--&gt;&cr;&lf;            &lt;div class = &quot;message&quot;&gt;&cr;&lf;              &lt;div class=&quot;user&quot;&gt;&cr;&lf;                [user]&cr;&lf;              &lt;/div&gt;&cr;&lf;              &lt;div class=&quot;text&quot;&gt;&cr;&lf;                [message]&cr;&lf;              &lt;/div&gt;&cr;&lf;            &lt;/div&gt;&cr;&lf;            &lt;!--/Message--&gt;&cr;&lf;&cr;&lf;        &lt;/div&gt;&cr;&lf;          &lt;div class=&quot;buddylist&quot;&gt;&cr;&lf;          &lt;h3&gt;User&lt;/h3&gt;&cr;&lf;          &lt;div&gt;&cr;&lf;            [users]&cr;&lf;          &lt;/div&gt;&cr;&lf;        &lt;/div&gt;&cr;&lf;    &lt;/div&gt;&cr;&lf;&cr;&lf;    &lt;div class = &quot;sendForm&quot;&gt;&cr;&lf;        &lt;form action=&quot;javascript:messageForm()&quot; name = &quot;message&quot;&gt;&cr;&lf;          &lt;input type=&quot;text&quot; name=&quot;Message&quot;&gt;&cr;&lf;          &lt;button&gt;Send&lt;/button&gt;&cr;&lf;        &lt;/form&gt;&cr;&lf;        &lt;div class = &quot;rightSpace&quot;&gt;&cr;&lf;        &lt;/div&gt;&cr;&lf;    &lt;/div&gt;&cr;&lf;&cr;&lf;  &lt;/div&gt;&cr;&lf;&cr;&lf;  &lt;div class=&quot;data&quot; id=&quot;data&quot;&gt;&cr;&lf;    &lt;di